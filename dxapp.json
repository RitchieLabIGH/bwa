{
  "name": "bwa",
  "title": "BWA",
  "summary": "Burrows-Wheeler Aligner",
  "description": "Burrows-Wheeler Aligner (BWA) is an efficient program that aligns relatively short nucleotide sequences against a long reference sequence such as the human genome.",
  "dxapi": "1.0.0",
  "inputSpec": [
    {"name": "reads", "class": "array:gtable", "type": "LetterReads"},
    {"name": "output name", "class": "string", "optional": true},
    {"name": "reference", "class": "record", "type": {"$or": ["ContigSet", "BwaLetterContigSetV2"]}},
    {"name": "algorithm", "class": "string", "default": "auto"},
    {"name": "aln_n", "class": "float", "optional": true},
    {"name": "aln_o", "class": "int", "optional": true},
    {"name": "aln_e", "class": "int", "optional": true},
    {"name": "aln_i", "class": "int", "optional": true},
    {"name": "aln_d", "class": "int", "optional": true},
    {"name": "aln_l", "class": "int", "optional": true},
    {"name": "aln_k", "class": "int", "optional": true},
    {"name": "aln_m", "class": "int", "optional": true},
    {"name": "aln_M", "class": "int", "optional": true},
    {"name": "aln_O", "class": "int", "optional": true},
    {"name": "aln_E", "class": "int", "optional": true},
    {"name": "aln_R", "class": "int", "optional": true},
    {"name": "aln_q", "class": "int", "optional": true},
    {"name": "aln_N", "class": "boolean", "default": false},
    {"name": "sampe_a", "class": "int", "optional": true},
    {"name": "sampe_o", "class": "int", "optional": true},
    {"name": "sampe_n", "class": "int", "optional": true},
    {"name": "sampe_N", "class": "int", "optional": true},
    {"name": "sampe_c", "class": "float", "optional": true},
    {"name": "sampe_s", "class": "boolean", "default": false},
    {"name": "samse_n", "class": "int", "optional": true},
    {"name": "sw_a", "class": "int", "optional": true},
    {"name": "sw_b", "class": "int", "optional": true},
    {"name": "sw_q", "class": "int", "optional": true},
    {"name": "sw_r", "class": "int", "optional": true},
    {"name": "sw_w", "class": "int", "optional": true},
    {"name": "sw_m", "class": "float", "optional": true},
    {"name": "sw_T", "class": "int", "optional": true},
    {"name": "sw_c", "class": "float", "optional": true},
    {"name": "sw_z", "class": "int", "optional": true},
    {"name": "sw_s", "class": "int", "optional": true},
    {"name": "sw_N", "class": "int", "optional": true},
    {"name": "chunk_size", "class": "int", "default": 25000000},
    {"name": "discard_unmapped_rows", "class": "boolean", "default": false}
  ],
  "outputSpec": [
    {"name": "mappings", "class": "gtable", "type": {"$and": ["LetterMappings", "Mappings", "gri"]}},
    {"name": "indexed_reference", "class": "record", "type": "BwaLetterContigSetV2"}
  ],
  "runSpec": {"file": "bwa.py", "interpreter": "python2.7", "execDepends": [{"name": "xz-utils"}]},
  "folder": "/bwa",
  "details": {
    "advancedInputs": [
      "algorithm", 
      "aln_n", 
      "aln_o", 
      "aln_e", 
      "aln_i", 
      "aln_d", 
      "aln_l", 
      "aln_k", 
      "aln_m", 
      "aln_M", 
      "aln_O", 
      "aln_E", 
      "aln_R", 
      "aln_q", 
      "sampe_a", 
      "sampe_o", 
      "sampe_n", 
      "sampe_N", 
      "sampe_c", 
      "sw_a", 
      "sw_b", 
      "sw_q", 
      "sw_r", 
      "sw_w", 
      "sw_m", 
      "sw_T", 
      "sw_c", 
      "sw_z", 
      "sw_s", 
      "sw_N", 
      "chunk_size", 
      "discard_unmapped_rows"
    ],
    "contactUrl": "http://wiki.dnanexus.com/Apps/bwa",
    "contactOrgs": ["org-dnanexus"],
    "repoUrl": "git@github.com:dnanexus/bwa.git",
    "upstreamUrl": "http://bio-bwa.sourceforge.net/",
    "upstreamVersion": "0.6.2",
    "upstreamLicenses": ["GPL3"],
    "citations": [
        "doi:10.1093/bioinformatics/btp324",
        "doi:10.1093/bioinformatics/btp698"
    ]
  },
  "version":"0.0.8",
  "categories": ["Alignment"],
  "resources": []
}
